{
  "name":  "Gremlin.Net",
  "title": "Gremlin.Net",
  "description": "Gremlin.Net is a cross-platform Gremlin Server driver for .NET that is written in C#. It uses WebSockets to communicate with Gremlin Server.",
  "authors": ["Florian Hockmann"],
  "version": "0.1.2-*",
  "dependencies": {
    "System.Collections": "4.0.11",
    "System.Linq": "4.1.0",
    "System.Runtime": "4.1.0",
    "System.Runtime.InteropServices": "4.1.0",
    "System.Threading": "4.0.11",
    "Newtonsoft.Json": "9.0.1",
    "System.Net.WebSockets": "4.0.0",
    "System.Net.WebSockets.Client": "4.0.0",
    "System.Collections.Concurrent": "4.0.12"
  },
  "frameworks": {
    "netstandard1.3": {
    }
  },
  "packOptions": {
    "summary": "A Gremlin driver for .NET",
    "tags": [ "gremlin", "tinkerpop", "tinkerpop3", "titan" ],
    "owners": [ "Florian Hockmann" ],
    "projectUrl": "https://github.com/FlorianHockmann/Gremlin.Net",
    "licenseUrl": "https://github.com/FlorianHockmann/Gremlin.Net/blob/master/LICENSE",
  },
  "buildOptions": {
    "xmlDoc": true
  }
}
